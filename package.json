{
  "name": "keepman",
  "main": "src/index.ts",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.ts",
    "preview": "bun x vite preview",
    "build": "bun x vue-tsc && bun x vite build",
    "dev": "bun x vue-tsc && bun x vite build && bun --watch src/index.ts",
    "db:studio": "bun x prisma --schema src/prisma/prisma.schema",
    "db:push": "bun x prisma --schema src/prisma/prisma.schema"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "5.11.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/joi": "^17.2.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "joi": "^17.12.2",
    "prisma": "^5.11.0",
    "vue": "^3.4.21"
  }
}
