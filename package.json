{
	"name": "keepman",
	"main": "src/index.ts",
	"module": "index.ts",
	"type": "module",
	"scripts": {
		"start": "bun run src/index.ts",
		"start:watch": "bun --watch run src/index.ts",
		"dev": "cd src/client && vue-tsc && vite build && cd ../../ && bun --watch src/index.ts",
		"preview": "cd src/client && vite preview && cd ../../",
		"client:build": "cd src/client && vue-tsc && vite build && cd ../../",
		"db:studio": "bun x prisma --schema src/prisma/prisma.schema",
		"db:push": "bun x prisma --schema src/prisma/prisma.schema"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@vitejs/plugin-vue": "^5.0.4",
		"typescript": "^5.2.2",
		"vite": "^5.1.6",
		"vue-tsc": "^1.8.27"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"@prisma/client": "5.11.0",
		"@types/cors": "^2.8.17",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.21",
		"@types/joi": "^17.2.3",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.18.3",
		"joi": "^17.12.2",
		"prisma": "^5.11.0",
		"vue": "^3.4.21"
	}
}
